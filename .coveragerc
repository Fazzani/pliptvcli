[run]
branch = True
source =
    pliptv/
omit =
    */itests/*
    **/__init__.py
[paths]
source =
    pliptv/
[report]
exclude_lines =
    raise NotImplementedError.*
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
